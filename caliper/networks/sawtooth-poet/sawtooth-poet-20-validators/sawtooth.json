{
  "caliper":{
    "blockchain": "sawtooth",
    "command" : {
      "start": "docker-compose -f network/sawtooth-poet/sawtooth-poet-20-validators/docker-compose.yaml up -d; sleep 100s",
      "end" : "docker-compose -f network/sawtooth-poet/sawtooth-poet-20-validators/docker-compose.yaml down"
    }
  },
  "sawtooth": {
    "batchBuilders": {
      "simple": {
        "1.0": "src/contract/sawtooth/simple/SimpleBatchBuilder.js"
      }
    },
    "network": {
      "restapi": {
        "urls": [
        "http://127.0.0.1:8000"
        ]
      },
      "validator": {
        "url": "tcp://127.0.0.1:4004"
      }
    }
  }
}
